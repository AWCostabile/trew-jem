{
  "name": "trew-jem-starter",
  "description": "A starter pack tailored towards fast development",
  "keywords": [
    "typescript",
    "react",
    "express",
    "webpack",
    "starter"
  ],
  "author": "Anthony Costabile",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "clean": "node bundling/clean",
    "build": " npm-run-all build:server build:client",
    "build:client": "cross-env SERVER=false webpack --config bundling",
    "build:server": "cross-env SERVER=true webpack --config bundling",
    "dev": "cross-env DEVELOPMENT=true npm-run-all clean start",
    "pretest": "tsc",
    "release": "npm-run-all clean test build",
    "prod": "cross-env PORT=80 npm-run-all release start",
    "start": "node services",
    "test": "jest --config jest.config.json --verbose",
    "update": "npm run test -- --updateSnapshot --onlyChanged --runTestsByPath",
    "update:all": "npm run update -- --all"
  },
  "dependencies": {
    "axios": "0.18.0",
    "body-parser": "1.19.0",
    "emotion": "9.2.12",
    "emotion-server": "9.2.12",
    "express": "4.16.4",
    "mobx": "5.9.4",
    "mobx-react": "5.4.3",
    "mobx-utils": "5.3.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-emotion": "9.2.12",
    "react-router-dom": "5.0.0",
    "universal-cookie": "3.1.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.2",
    "@types/react-test-renderer": "16.8.1",
    "@types/uuid": "3.4.4",
    "cross-env": "5.2.0",
    "dotenv": "7.0.0",
    "file-loader": "3.0.1",
    "fork-ts-checker-webpack-plugin": "1.0.2",
    "jest": "24.7.1",
    "jest-emotion": "9.2.11",
    "memory-fs": "0.4.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.17.0",
    "react-test-renderer": "16.8.6",
    "require-from-string": "2.0.2",
    "source-map-loader": "0.2.4",
    "ts-jest": "24.0.2",
    "ts-loader": "5.3.3",
    "tslint": "5.16.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.3",
    "url-loader": "1.1.2",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-middleware": "3.6.2",
    "webpack-hot-middleware": "2.24.3",
    "webpack-hot-server-middleware": "0.6.0"
  }
}
